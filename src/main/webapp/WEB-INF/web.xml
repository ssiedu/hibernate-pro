<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveStudent</servlet-name>
  	<display-name>SaveStudent</display-name>
  	<description></description>
  	<servlet-class>com.ssi.SaveStudent</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RemoveStudent</servlet-name>
  	<display-name>RemoveStudent</display-name>
  	<description></description>
  	<servlet-class>com.ssi.RemoveStudent</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SaveStudent</servlet-name>
  	<url-pattern>/SaveStudent</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RemoveStudent</servlet-name>
  	<url-pattern>/RemoveStudent</url-pattern>
  </servlet-mapping>
</web-app>
